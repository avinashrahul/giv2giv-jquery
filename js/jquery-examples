Subscribe to endowment
curl -X POST -H "Content-Type: application/json" -H 'Authorization: Token token=l6M4VWVgiVFhT4IbgSkYwA' -d '{"amount":"5", "endowment_id":"1"}' http://localhost:3000/api/donors/payment_accounts/1/donate_subscription.json

jQuery.ajax({
  url: '/api/donors/payment_accounts/{payment_account_id}/donate_subscription.json',
  method: 'POST',
  headers: {
    'Authentication' : 'Token token=' + token
  },
  data: {
    amount: '5',
    endowment_id: $endowment_id
  },
  success: function(data) {
#Response: {"donation":{"amount":5.0,"endowment_id":1,"created_at":"2013-09-24T04:56:48Z","id":1,"payment_account_id":1,"transaction_id":null,"transaction_processor":"stripe","transaction_type":"subscription","updated_at":"2013-09-24T04:56:48Z"}}
  },
  error: function (data, statusCode) {
  }
});



