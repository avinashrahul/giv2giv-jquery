
Create donor
curl -X POST -H "Content-Type: application/json" -d '{ "email":"email@domain.com","password":"password","name":"donor name" }' http://localhost:3000/api/donors.json

jQuery.ajax({
  url: '/api/donors.json',
  method: 'POST',
  data: {
    email: $email,
    password: $password,
    name: "Donor Name"
  },
  success: function(data) {
# Response: {"donor":{"address":null,"city":null,"country":null,"created_at":"2013-09-24T04:34:59Z","email":"email@domain.com","facebook_id":null,"id":1,"name":"donor name","phone_number":null,"state":null,"updated_at":"2013-09-24T04:34:59Z","zip":null}}
  },
  error: function (data, statusCode) {
  }
});



Authenticate
curl -X POST -H "Content-Type: application/json" -d '{"email":"email@domain.com","password":"password"}' http://localhost:3000/api/sessions/create.json

jQuery.ajax({
  url: '/api/sessions/create.json',
  method: 'POST',
  data: {
    email: $email,
    password: $password
  },
  success: function(data) {
#Response: {"session":{"created_at":"2013-09-24T04:35:36Z","session_id":1,"token":"l6M4VWVgiVFhT4IbgSkYwA","updated_at":"2013-09-24T04:35:36Z"}}

  },
  error: function (data, statusCode) {
  }
});



Create (stripe) payment account
curl -X POST -H "Content-Type: application/json" -H 'Authorization: Token token=l6M4VWVgiVFhT4IbgSkYwA' -d '{"processor": "stripe", "stripeToken":"tok_102dPs2mGsIlOBKnRLRez6VK"}' http://localhost:3000/api/donors/payment_accounts.json

jQuery.ajax({
  url: '/api/donors/payment_accounts.json',
  method: 'POST',
  headers: {
    'Authentication' : 'Token token=' + token
  },
  data: {
    processor: 'stripe',
    stripeToken: $stripeToken
  },
  success: function(data) {
#Response: {"payment_account":{"created_at":"2013-09-24T04:40:15Z","donor_id":1,"id":1,"processor":"stripe","requires_reauth":false,"stripe_cust_id":"cus_2d3jQymPJ1qWWR","updated_at":"2013-09-24T04:40:15Z"}}
  },
  error: function (data, statusCode) {
  }
});



