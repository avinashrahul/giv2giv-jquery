Get donations (for statement)
curl -X GET -H "Content-Type: application/json" -H 'Authorization: Token token=6MyKLFF_11ab8WrthtAT5Q' -d '{"endowment_id":"1", "start_date":"2013-10-15", "end_date":"2015-10-17"}' http://localhost:3000/api/donors/endowments.json

jQuery.ajax({
  url: '/api/donors/endowments.json',
  method: 'GET',
  headers: {
    'Authentication' : 'Token token=' + token
  },
  data: {
    endowment_id: $endowment_id
    start_date: "2013-01-01",
    end_date: "2013-12-31" 
  },
  success: function(data) {
#Response: {"donations":[{"donation":{"created_at":"2013-12-01T01:52:34Z","donor_id":6,"endowment_id":11,"gross_amount":333.0,"id":3,"net_amount":323.043,"payment_account_id":2,"shares_added":"0.00323043","transaction_fees":9.957,"updated_at":"2013-12-01T01:52:34Z"}}],"total":333.0}
