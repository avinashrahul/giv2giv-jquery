Create endowment
curl -X POST -H "Content-Type: application/json" -H 'Authorization: Token token=l6M4VWVgiVFhT4IbgSkYwA' -d '{"name":"Build Houses","visibility":"public", "description":"Your description","minimum_donation_amount":"1","charity_id":"1,24,6,4,3"}' http://localhost:3000/api/endowment.json

jQuery.ajax({
  url: '/api/endowment.json',
  method: 'POST',
  headers: {
    'Authentication' : 'Token token=' + token
  },
  data: {
    name: $endowmentName,
    visibility: "public" || "private",
    description: $endowmentDescription,
    minimum_donation_amount: $minimum_donation_amount,
    charity_ids: "comma,separated,list"
  },
  success: function(data) {
#Response: {"endowment":{"created_at":"2013-11-16T13:33:47Z","description":"Your description","donor_id":1,"visibility":"public","id":1,"minimum_donation_amount":1.0,"name":"Build 2 Houses","updated_at":"2013-11-16T13:33:47Z","charities":[{"active":"true","id":1,"name":"1103 MARKET STREET FOUNDATION"},{"active":"false","id":3,"name":"1916 FOUNDATION"},{"active":"true","id":4,"name":"1ST LEGION PROVISIONAL ARMY OF CONFEDERATE STATES"},{"active":"false","id":6,"name":"214 PHILANTHROPIC FOUNDATION INC"},{"active":"true","id":24,"name":"A FATHERS HEART MINISTRIES INC"}]}}
  },
  error: function (data, statusCode) {
  }
});



