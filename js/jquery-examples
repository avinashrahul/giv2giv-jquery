jQuery.ajax({
  url: '/api/sessions/destroy.json',
  method: 'POST',
  headers: {
    'Authentication' : 'Token token=' + token
  },
  data: {
  },
  success: function(data) {
#Response: {"message":"Successfuly remove your session"}
  },
  error: function (data, statusCode) {
  }
});


